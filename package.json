        "javascript-stringify": "^2.0.1",
        "jira-connector": "^3.1.0",
        "luxon": "^1.25.0",
        "mobx": "5.15.7",
        "mobx-react-lite": "2.2.2",
        "mocha": "^8.1.3",
        "moment": "2.29.0",
        "moment-countdown": "^0.0.3",
        "moment-timezone": "^0.5.31",
        "notistack": "1.0.0",
        "null-loader": "4.0.0",
        "oracledb": "5.0.0",
        "pretty-quick": "3.0.2",
        "query-string": "6.13.4",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "reflect-metadata": "^0.1.13",
        "sort-keys": "^4.0.0",
        "sql-prettier": "^0.1.6",
        "styled-components": "^5.1.0",
        "typescript": "4.0.3",
        "uuid": "^8.1.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-node-externals": "2.5.2",
        "ws": "^7.2.3"
    },
    "private": true,
    "sideEffects_DISABLED": [],
    "nyc": "^15.0.0",
    "plainDependencies": true,
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "trailingComma": "es5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
